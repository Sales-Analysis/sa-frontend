{
  "name": "sa-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eslint": "",
    "eslint:fix": "",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public",
    "intl": "npm run intl:extract && npm run intl:compile",
    "intl:extract": "dotenv -c .env.local -- formatjs extract 'src/**/*.ts*' --id-interpolation-pattern '[sha512:contenthash:base64:6]' > src/lang/ru-RU.json",
    "prepare": "husky install",
    "graphql-faker": "graphql-faker ./src/apollo/schema.graphql -p 8080",
    "apollo:codegen": "apollo client:codegen src/types -c ./apollo.config.js --target=typescript --outputFlat",
    "generate": "graphql-codegen"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@consta/uikit": "^2.2.0",
    "@graphql-tools/mock": "^8.1.4",
    "@sales-analysis/sa-frontend-configs": "^0.1.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.20.19",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@visx/group": "^1.17.1",
    "@visx/mock-data": "^1.7.0",
    "@visx/scale": "^1.14.0",
    "@visx/shape": "^1.17.1",
    "babel-loader": "8.1.0",
    "graphql": "^15.5.1",
    "graphql.macro": "^1.4.2",
    "react": "^16.8.6",
    "react-collapsible": "^2.8.3",
    "react-dom": "^16.8.6",
    "react-intl": "^5.20.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.5",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.2.29",
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/typescript": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^2.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.0.0",
    "@graphql-tools/graphql-file-loader": "^7.0.0",
    "@graphql-tools/load": "^7.0.0",
    "@graphql-tools/schema": "^8.0.3",
    "@graphql-tools/stitch": "^8.0.3",
    "@graphql-tools/url-loader": "^7.0.3",
    "@storybook/addon-actions": "^6.3.5",
    "@storybook/addon-essentials": "^6.3.5",
    "@storybook/addon-links": "^6.3.5",
    "@storybook/node-logger": "^6.3.5",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.5",
    "@types/faker": "^5.5.7",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/parser": "^4.28.4",
    "autoprefixer": "^10.3.1",
    "classnames": "^2.3.1",
    "dotenv-cli": "^4.0.0",
    "eslint-plugin-formatjs": "^2.17.2",
    "faker": "^5.5.3",
    "husky": "^7.0.0",
    "plop": "^2.7.4",
    "postcss": "^8.3.6",
    "postcss-loader": "^4.3.0",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  }
}
