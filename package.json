{
  "name": "sa-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eslint": "",
    "eslint:fix": "",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public",
    "intl": "npm run intl:extract && npm run intl:compile",
    "intl:extract": "dotenv -c .env.local -- formatjs extract 'src/**/*.ts*' --id-interpolation-pattern '[sha512:contenthash:base64:6]' > src/lang/ru-RU.json",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@consta/uikit": "^2.2.0",
    "@sales-analysis/sa-frontend-configs": "0.1.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "graphql": "^15.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.5",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.2.29",
    "@storybook/addon-actions": "^6.3.5",
    "@storybook/addon-essentials": "^6.3.5",
    "@storybook/addon-links": "^6.3.5",
    "@storybook/node-logger": "^6.3.5",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.5",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/parser": "^4.28.4",
    "autoprefixer": "^10.3.1",
    "classnames": "^2.3.1",
    "dotenv-cli": "^4.0.0",
    "eslint-plugin-formatjs": "^2.17.2",
    "husky": "^7.0.0",
    "plop": "^2.7.4",
    "postcss": "^8.3.6",
    "postcss-loader": "^4.3.0",
    "postcss-nested": "^5.0.5",
    "react-app-rewired": "^2.1.8"
  },
  "peerDependencies": {
    "postcss": "^8.3.6"
  }
}
